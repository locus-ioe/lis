{% extends 'base.html.twig' %}

{% block title %} {{ title }} Project | {{ parent() }} {% endblock %}

{% block body %}
  <div id="wrapper">
    <div id="container">
      <div id="welcome">
        <h1>{{ title }} Project</h1>
        {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
        {{ form_errors(form) }}
        <table>
        <tr>
          <td>{{ form_label(form.title) }}</td>
          <td>{{ form_widget(form.title) }}</td>
          <td>{{ form_errors(form.title) }}</td>
        </tr>
        <tr>
          <td>{{ form_label(form.description) }}</td>
          <td>{{ form_widget(form.description) }}</td>
          <td>{{ form_errors(form.description) }}</td>
        </tr>
        <tr>
          <td>{{ form_label(form.type) }}</td>
          <td>{{ form_widget(form.type) }}</td>
          <td>{{ form_errors(form.type) }}</td>
        </tr>
        <tr>
          <td>{{ form_label(form.detail) }}</td>
          <td>{{ form_widget(form.detail) }}</td>
          <td>{{ form_errors(form.detail) }}</td>
        </tr>
        <tr>
          <td>{{ form_label(form.theme) }}</td>
          <td>{{ form_widget(form.theme) }}</td>
          <td>{{ form_errors(form.theme) }}</td>
        </tr>
        <tr>
          <td>{{ form_label(form.stall) }}</td>
          <td>{{ form_widget(form.stall) }}</td>
          <td>{{ form_errors(form.stall) }}</td>
        </tr>
        <tr>
          <td>{{ form_label(form.events) }}</td>
          <td>{{ form_widget(form.events) }}</td>
          <td>{{ form_errors(form.events) }}</td>
        </tr>
        <tr>
          <td>{{ form_label(form.members) }}</td>
          <td>{{ form_widget(form.members) }}</td>
          <td>{{ form_errors(form.members) }}</td>
        </tr>
        </table>
        {{ form_end(form) }}
      </div>
    </div>
  </div>
{% endblock %}
